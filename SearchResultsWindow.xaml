<Window x:Class="lab5.SearchResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Search results" Height="300" Width="400" ResizeMode="NoResize">
    <StackPanel>
        <TextBox Name="searchResultsTextBox" VerticalScrollBarVisibility="Auto" IsReadOnly="True"/>
        <Button Content="Close" Click="CloseButton_Click" HorizontalAlignment="Right" Margin="0,5"/>
    </StackPanel>
</Window>
